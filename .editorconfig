root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
continuation_indent_size = 4
curly_bracket_next_line = false
trim_trailing_whitespace = true
max_line_length = 140
spaces_around_operators = true
spaces_around_brackets = both
wildcard_import_limit = 10000

[*.properties]
charset = latin1